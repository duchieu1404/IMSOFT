<!-- Content Header (Page header)-->
<section class="content-header">
  <h1>List Cashout<small>Cashout</small></h1>
  <ol class="breadcrumb">
    <li><a href="#"><i class="fa fa-dashboard"></i>Cashout</a></li>
  </ol>
</section>
<!-- Main content-->
<section class="content">
  <div class="col-md-12 col-sm-12 col-xs-12">
    <div class="x_panel">
      <div class="x_title">
        <h3>CHARGE CARD<small> - Search</small></h3>
        <div class="clearfix"></div>
      </div>

      <div class="x_content">
        <br />
        <form id="demo-form2" data-parsley-validate class="form-horizontal form-label-left">

          <!-- === -->

          <div class="form-group">
            <label class="control-label col-md-2 col-sm-3 col-xs-4">User ID</label>

            <div class="col-md-3 col-sm-9 col-xs-12">
              <input id="searchUserId" class="form-control col-md-5 col-xs-10" required="required" type="text">
            </div>

            <label class="control-label col-md-2 col-sm-3 col-xs-4">Full name</label>

            <div class="col-md-4 col-sm-9 col-xs-12">
              <input id="searchFullName"type="text" class="form-control  col-md-5 col-xs-10" placeholder="">
            </div>
          </div>

          <!-- === -->

          <div class="form-group">
            <label class="control-label col-md-2 col-sm-3 col-xs-4">From <span class="required">*</span></label>

            <div class="col-md-3 col-sm-9 col-xs-12">
              <input id="searchStartTime" class="date-picker form-control col-md-5 col-xs-10" required="required" type="text">
            </div>

            <label class="control-label col-md-2 col-sm-3 col-xs-4">To <span class="required">*</span></label>

            <div class="col-md-4 col-sm-9 col-xs-12">
              <input id="searchEndTime" class="date-picker form-control col-md-5 col-xs-10" required="required" type="text">
            </div>
          </div>

          <!-- === -->
          <div class="form-group">
            <label class="control-label col-md-2 col-sm-3 col-xs-4">Card Code</label>

            <div class="col-md-3 col-sm-9 col-xs-12">
              <input id="searchCardCode" class="form-control col-md-5 col-xs-10" required="required" type="text">
            </div>

            <label class="control-label col-md-2 col-sm-3 col-xs-4">Card Seri</label>

            <div class="col-md-4 col-sm-9 col-xs-12">
              <input id="searchCardSeri"type="text" class="form-control  col-md-5 col-xs-10" placeholder="">
            </div>
          </div>
          <!-- === -->

          <div class="form-group">
            <label class="control-label col-md-2 col-sm-3 col-xs-4">Nhà mạng</label>

            <div class="col-md-3 col-sm-9 col-xs-12">
              <select class="form-control">
                <option>Tất cả</option>
                <option>Viettel</option>
                <option>VinaPhone</option>
                <option>MobilePhone</option>
                <option>VietNamMobile</option>
              </select>
            </div>

            <label class="control-label col-md-2 col-sm-3 col-xs-4">Trạng thái</label>

            <div class="col-md-4 col-sm-9 col-xs-12">
              <select class="form-control">
                <option>Tất cả</option>
                <option>Đang chờ xử lý</option>
                <option>Thành công</option>
                <option>Thất bại</option>
              </select>
            </div>
          </div>

          <div class="modal-footer">
            <div class="col-md-6 col-sm-9 col-xs-12">
              <input type="submit" class="btn btn-success" value="Search"/>
            </div>
          </div>

        </form>

      </div>
    </div>
  </div>

